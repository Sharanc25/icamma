/*============================================== Messages =============================================*/
div.messages.status li:last-child {
	display: none;
}
/*============================================== Messages - End =============================================*/



/*==============================================Horizontal Tabs=============================================*/
div.horizontal-tabs {
	border-top: none;
}
.horizontal-tabs ul.horizontal-tabs-list li:hover, .horizontal-tabs ul.horizontal-tabs-list li:focus {
	background-color: #fff;
	box-shadow: 5px inset #fff;
}
.horizontal-tabs ul.horizontal-tabs-list li a:hover, .horizontal-tabs ul.horizontal-tabs-list li:hover {
	background-color: #fff;
}
.horizontal-tabs ul.horizontal-tabs-list {
	border-bottom: 1px solid #ccc;
	border-right: none;
	border-collapse: collapse;
	list-style: outside none none;
	background: #fff;
	white-space: nowrap;
	width: 100.1%;
}
.horizontal-tabs ul.horizontal-tabs-list li {
	border-top: 1px solid #ccc;
	margin-right: 10px;
	display: inline;
	z-index: 999;
}
.horizontal-tabs ul.horizontal-tabs-list li.last {
	border-left: 1px solid #ccc;
}
