/*============================================== Header =============================================*/
#branding {
    padding: 34px 0 35px;
}
header.page-title {
    background: #4e5066;
    color: #fff;
    margin-bottom: 10px;
}
h1#page-title, .in-maintenance h1#page-title {
    padding: 14px;
    margin: 0;
}
/* Slogan */
h2#site-slogan {
    font-size: 23px;
}
#site-slogan::selection {
    color: #fff;
}
#site-slogan::-moz-selection {
    color: #fff;
}
span#conference-dates::selection {
    color: #fff;
}
span#conference-dates::moz-selection {
    color: #fff;
}
#site-slogan span#conference-dates {
    display: block;
}
#site-name a::selection {
    color: #fff;
}
#site-name a::-moz-selection {
    color: #fff;
}
/*============================================== Header - End =============================================*/

.content-style {
	border: none;
	background: none;
}
#block-system-help {
	background: none;
	border: none;
	margin-top: 25px;
}
#block-search-form {
	float: right !important;
}
#search-block-form {
	margin: 0.4em 0 0 0;
}
#block-search-form input.form-text {
	width: 225px;
	margin-bottom: 0;
}
section#main-content {
	width: 100%;
}
div#footer-wrapper div.block-content {
	text-align: center;
}
/* Menu Highlight Fix for IE*/
.nav .block-menu ul li:focus, .nav .block-menu ul li:hover, .nav .block-menu ul li:active {
	background: #e25402;
}
.nav .block-menu ul a:visited {
	background: none;
}
#footer-wrapper, #footer-panels-wrapper {
	background-color: #2e2e2d;
	color: #fff;
	margin-bottom: 0;
}
#footer-wrapper ul.menu {
	text-align: center;
}
#footer-wrapper ul.menu li {
	display: inline;
	padding: 0 15px;
	border-right: 1px #fff solid;
}
#footer-wrapper ul.menu li.last {
	border: none;
}
/*============================================== Home Page =============================================*/
div.flex-caption {
	text-align: center;
	font-weight: bold;
}
div#about-amma-2015 div#preamble {
	text-indent: 50px;
	text-align: justify;
}
div#about-amma-2015 h3 {
