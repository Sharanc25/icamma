	text-align: center;
	background-color: #4e5066;
	padding: 5px 0;
	color: #fff;
}
div#about-amma-2015 span.minor-heading {
	/*color: #e25400;*/
	font-weight: bold;
}
div#about-amma-2015 a.submit-abstract-link {
	background-color: #2e2e2d;
	color: #fff;
	padding: 1em 2em;
}
div#about-amma-2015 a.submit-abstract-link:hover {
	background-color: #e25400;
}
/*============================================== Home Page - End =============================================*/


/*============================================== Blocks =============================================*/
div.region-highlighted div.block-content {
